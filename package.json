{
  "name": "lca-collect",
  "license": "Apache-2.0",
  "private": true,
  "description": "Frontend for LCAcollect",
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4200 --host",
    "build": "tsc && vite build",
    "preview": "vite preview --port 4200",
    "e2e": "start-server-and-test dev http://localhost:4200 cy:run",
    "e2e:open": "cypress open",
    "cy:run": "cypress run",
    "lint": "eslint src/**/*.{tsx,ts}",
    "lint:fix": "eslint --fix src/**/*.{tsx,ts}",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "codegen": "graphql-codegen --config codegen.yml",
    "format:fix": "prettier --write src/",
    "format": "prettier --check src/",
    "fix": "npm run lint:fix && npm run format:fix"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.30.0",
    "@azure/msal-react": "^1.4.9",
    "@fontsource/roboto": "^4.5.8",
    "@lcacollect/assembly": "^0.2.1",
    "@lcacollect/components": "^1.1.5",
    "@lcacollect/core": "^1.0.5",
    "@lcacollect/documentation": "^1.2.2",
    "@lcacollect/project": "^1.2.2",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/x-date-pickers-pro": "^5.0.14",
    "@mui/x-license-pro": "^5.17.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-charts": "^4.0.0",
    "react-router-dom": "^6.4.2",
    "styled-components": ">= 5"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.2",
    "@babel/eslint-parser": "^7.21.3",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.16",
    "@graphql-codegen/typescript-resolvers": "^2.7.2",
    "@graphql-tools/mock": "^8.7.6",
    "@graphql-tools/schema": "^9.0.4",
    "@lcacollect/e2e-testing": "^1.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@tsconfig/vite-react": "^1.0.1",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/ui": "^0.28.2",
    "copyfiles": "^2.4.1",
    "cypress": "^12.1.0",
    "cypress-vite": "^1.2.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest-environment-jsdom": "^29.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "styled-components": "^5.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.7",
    "vitest": "^0.25.8"
  },
  "pre-commit": [
    "lint",
    "format"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
